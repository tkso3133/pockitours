#blueLocationInformationBar { width: 100%; height: 32px; background-color: #27beda; }
#imgBlueOpenBook { float: left; height: 17px; width: 17px; margin: 8px 0 0 5px; }
#lblLocationName { float: left; margin: 7px 0 0 10px; font-size: 17px; }
#locationDescription { width: 95%; margin: 10px auto; }
#bottom { background: url(../img/bottom_grey_fade.png) repeat-x; height: 50px; }
#bottom a { color: grey; width: 100%; height: 100%; }
#photos { height: 60%; width: 100%; z-index: -1; }
#audioButton { float: left; height: 35px; width: 50px; margin: 0px 0 0 12%; }
